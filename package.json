{
  "name": "ncc-ts-template",
  "version": "1.0.0",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "node ./build/index.js",
    "build:encode": "node ./build/index.js -e",
    "build:pkg": "node ./build/index.js -b",
    "format": "prettier --write '**/*.{js,ts,jsx,tsx,json,yml,yaml,md,html}' --ignore-path .gitignore",
    "lint": "eslint ."
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@vercel/ncc": "^0.38.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "nodemon": "^3.1.0"
  }
}
